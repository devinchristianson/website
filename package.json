{
  "name": "devin-christianson-website",
  "description": "A personal website",
  "version": "0.103.0",
  "private": true,
  "scripts": {
    "build": "qwik build && jampack ./dist",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/static/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "wrangler pages publish ./dist --branch preview",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.18.0",
    "@builder.io/qwik-city": "^1.18.0",
    "@divriots/jampack": "^0.22.0",
    "@fontsource/inter": "^5.0.15",
    "@tailwindcss/typography": "^0.5.10",
    "@types/eslint": "8.44.6",
    "@types/markdown-it": "^13.0.5",
    "@types/node": "latest",
    "@typescript-eslint/eslint-plugin": "6.10.0",
    "@typescript-eslint/parser": "6.10.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.57.0",
    "eslint-plugin-qwik": "^1.18.0",
    "feed": "^4.2.2",
    "gray-matter": "^4.0.3",
    "imagetools-core": "^5.1.1",
    "markdown-it": "^13.0.2",
    "node-fetch": "3.3.2",
    "postcss": "^8.4.31",
    "prettier": "3.0.3",
    "tailwindcss": "^3.3.5",
    "typescript": "5.2.2",
    "vite": "^7.3.0",
    "vite-imagetools": "^9.0.2",
    "vite-tsconfig-paths": "6.0.3",
    "wrangler": "latest"
  },
  "overrides": {
    "@divriots/jampack": {
      "cheerio": "1.0.0-rc.12"
    }
  },
  "engines": {
    "node": ">=15.0.0"
  },
  "type":"module"
}
